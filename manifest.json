{
"manifest_version": 2,
    "name": "Amazon Wishlist Sum",
    "description": "Sum up the prices of an Amazon wishlist in the wishlist view",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "https://www.amazon.com/gp/registry/wishlist/*",
                "http://www.amazon.com/gp/registry/wishlist/*",
                "https://www.amazon.co.uk/gp/registry/wishlist/*",
                "http://www.amazon.co.uk/gp/registry/wishlist/*"
            ],
            "js": [
                "jquery-2.2.3.min.js", "content.js"
            ],
            "run_at": "document_end",
            "all_frames": false
        }
    ]
}
